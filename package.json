{
	"name": "wikidata-sdk-got",
	"version": "2.0.0",
	"description": "Run wikidata-sdk requests from NodeJS without handling urls",
	"license": "MIT",
	"repository": "EdJoPaTo/wikidata-sdk-got",
	"author": {
		"name": "EdJoPaTo",
		"email": "edjopato@gmail.com",
		"url": "https://edjopato.de"
	},
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"prepack": "npm run build",
		"test": "tsc --sourceMap && xo && nyc ava"
	},
	"main": "dist/source",
	"types": "dist/source",
	"files": [
		"dist/source",
		"!*.test.*"
	],
	"keywords": [
		"wikidata",
		"sparql"
	],
	"dependencies": {
		"array-filter-unique": "^2.1.0",
		"got": "^11.1.0",
		"wikibase-types": "^0.1.0",
		"wikidata-sdk": "^7.4.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.7.0",
		"ava": "^3.8.1",
		"del-cli": "^3.0.0",
		"nyc": "^15.0.1",
		"typescript": "^3.3.4000",
		"xo": "^0.30.0"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"include": [
			"dist/source",
			"source"
		],
		"exclude": [
			"**/*.test.ts"
		],
		"all": true,
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"rules": {
			"@typescript-eslint/prefer-readonly-parameter-types": "off",
			"ava/no-ignored-test-files": "off",
			"unicorn/no-fn-reference-in-iterator": "off"
		}
	}
}
