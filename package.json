{
	"name": "wikidata-sdk-got",
	"version": "2.1.0",
	"description": "Run wikidata-sdk requests from NodeJS without handling urls",
	"keywords": [
		"wikidata",
		"sparql"
	],
	"license": "MIT",
	"repository": "EdJoPaTo/wikidata-sdk-got",
	"author": {
		"name": "EdJoPaTo",
		"email": "wikibase-npm-package@edjopato.de",
		"url": "https://edjopato.de"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"prepack": "npm run build",
		"test": "tsc --sourceMap && xo && nyc ava"
	},
	"engines": {
		"node": ">=10"
	},
	"dependencies": {
		"array-filter-unique": "^2.1.0",
		"got": "^11.1.0",
		"wikibase-types": "^0.2.1",
		"wikidata-sdk": "^7.4.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^1.0.2",
		"ava": "^3.8.1",
		"del-cli": "^3.0.0",
		"nyc": "^15.0.1",
		"typescript": "^4.1.5",
		"xo": "^0.38.1"
	},
	"files": [
		"dist/source",
		"!*.test.*"
	],
	"main": "dist/source",
	"types": "dist/source",
	"nyc": {
		"all": true,
		"extension": [
			".ts"
		],
		"include": [
			"dist/source",
			"source"
		],
		"exclude": [
			"**/*.d.ts",
			"**/*.test.ts"
		],
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"rules": {
			"@typescript-eslint/prefer-readonly-parameter-types": "error",
			"ava/no-ignored-test-files": "off"
		},
		"overrides": [
			{
				"files": [
					"**/*.test.*",
					"test/**/*.*"
				],
				"rules": {
					"@typescript-eslint/prefer-readonly-parameter-types": "off"
				}
			}
		]
	}
}
