{
	"name": "wikidata-sdk-got",
	"version": "0.0.0",
	"description": "Run wikidata-sdk requests from NodeJS without handling urls",
	"license": "MIT",
	"repository": "EdJoPaTo/wikidata-sdk-got",
	"author": {
		"name": "Edgar Toll",
		"email": "edjopato@gmail.com",
		"url": "edjopato.de"
	},
	"engines": {
		"node": ">=6"
	},
	"scripts": {
		"build": "del dist && tsc",
		"prepack": "npm run build",
		"test": "xo && nyc ava"
	},
	"main": "dist",
	"types": "dist",
	"files": [
		"dist",
		"!*.test.*"
	],
	"keywords": [
		"wikidata",
		"sparql"
	],
	"dependencies": {
		"got": "^9.6.0"
	},
	"peerDependencies": {
		"wikidata-sdk": "^6.1.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.2.1",
		"@types/node": "^11.11.7",
		"@typescript-eslint/eslint-plugin": "^1.5.0",
		"ava": "^1.4.0",
		"del-cli": "^1.1.0",
		"eslint-config-xo-typescript": "^0.8.0",
		"nyc": "^13.3.0",
		"ts-node": "^8.0.3",
		"typescript": "^3.3.4000",
		"wikidata-sdk": "^6.1.0",
		"xo": "^0.24.0"
	},
	"ava": {
		"babel": false,
		"compileEnhancements": false,
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"include": [
			"source"
		],
		"exclude": [
			"**/*.test.ts"
		],
		"all": true,
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"extends": "xo-typescript",
		"extensions": [
			"ts"
		],
		"rules": {
			"@typescript-eslint/promise-function-async": "off",
			"ava/no-ignored-test-files": "off",
			"import/export": "off"
		}
	}
}
