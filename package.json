{
	"name": "wikidata-sdk-got",
	"version": "2.1.0",
	"description": "Run wikidata-sdk requests from NodeJS without handling urls",
	"keywords": [
		"wikidata",
		"sparql"
	],
	"license": "MIT",
	"repository": "EdJoPaTo/wikidata-sdk-got",
	"author": {
		"name": "EdJoPaTo",
		"email": "wikibase-npm-package@edjopato.de",
		"url": "https://edjopato.de"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"prepack": "npm run build",
		"test": "tsc --sourceMap && xo && nyc ava"
	},
	"type": "commonjs",
	"engines": {
		"node": ">=12"
	},
	"dependencies": {
		"array-filter-unique": "^2.1.0",
		"got": "^11.1.0",
		"wikibase-types": "^0.4.0",
		"wikidata-sdk": "^8.0.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^2.0.0",
		"ava": "^4.0.1",
		"del-cli": "^4.0.0",
		"nyc": "^15.0.1",
		"typescript": "^4.2.3",
		"xo": "^0.49.0"
	},
	"files": [
		"dist/source",
		"!*.test.*"
	],
	"main": "./dist/source/index.js",
	"types": "./dist/source/index.d.ts",
	"nyc": {
		"all": true,
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"rules": {
			"@typescript-eslint/naming-convention": "off",
			"@typescript-eslint/no-require-imports": "off",
			"@typescript-eslint/no-unsafe-assignment": "off",
			"@typescript-eslint/no-unsafe-call": "off",
			"@typescript-eslint/no-unsafe-return": "off",
			"@typescript-eslint/prefer-readonly-parameter-types": "off",
			"ava/no-ignored-test-files": "off",
			"import/extensions": [
				"error",
				"never"
			],
			"unicorn/prefer-module": "off"
		}
	}
}
